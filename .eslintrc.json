{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "cypress/globals": true
  },
  "extends": ["eslint:recommended", "plugin:cypress/recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["cypress"],
  "rules": {
    "prettier/prettier": ["error"]
  }
}
